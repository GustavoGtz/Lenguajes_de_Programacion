#lang racket

(require rackunit
         rackunit/text-ui
         "lexer.rkt")

(define regex-tests
  (test-suite
   "Pruebas para lexer.rkt"
   (test-case "Lexer"
              (equal?
               (lex-from-string (list->string '((define x 4) +54)))
               (list '(token 'define #f lex-line col))))))

(run-tests regex-tests 'verbose)
 
